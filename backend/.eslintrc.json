{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
    },
    "env": {
        "node": true
    },
    "extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended", "prettier/@typescript-eslint"],
    "rules": {
        "linebreak-style": 0,
        "no-console": "warn",
        "semi": ["error", "never"],
        "@typescript-eslint/no-empty-function": "warn",
        "no-unused-vars": [
            "error",
            {
                "args": "none"
            }
        ],
        "prettier/prettier": [
            "error", { "endOfLine": "auto" }
        ]
    }
}
