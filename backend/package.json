{
    "name": "ezset",
    "version": "1.0.0",
    "description": "",
    "repository": "https://github.com/Tekiter/EZSET",
    "license": "MIT",
    "main": "dist/index",
    "scripts": {
        "prestart": "npm run -s build",
        "start": "node dist/index.js",
        "dev": "nodemon src/index.ts --exec \"ts-node -r dotenv/config\"",
        "clean": "rimraf dist",
        "build": "npm run clean && tsc --build",
        "test": "jest --watch",
        "lint": "eslint src",
        "api": "apidoc -o apidoc/ -i src/api/v1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.(js|ts)": [
            "eslint src",
            "prettier --write",
            "git add"
        ]
    },
    "keywords": [
        "express",
        "babel",
        "boilerplate",
        "scaffold",
        "es6",
        "es2015",
        "es2016",
        "es2017",
        "jest",
        "eslint"
    ],
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "bcrypt-nodejs": "^0.0.3",
        "body-parser": "^1.18.2",
        "cachegoose": "^8.0.0",
        "connect-history-api-fallback": "^1.6.0",
        "elastic-apm-node": "^3.12.1",
        "express": "^4.16.4",
        "express-validator": "^6.2.0",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.24.0",
        "mongoose": "^5.7.8",
        "mongoose-auto-increment": "^5.0.1",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "nocache": "^2.1.0",
        "random-number-csprng": "^1.0.2",
        "rimraf": "^2.6.3",
        "socket.io": "^2.3.0"
    },
    "devDependencies": {
        "@types/bcrypt-nodejs": "^0.0.31",
        "@types/connect-history-api-fallback": "^1.3.4",
        "@types/elastic-apm-node": "^2.7.0",
        "@types/express": "^4.17.11",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/mongoose": "^5.10.5",
        "@types/mongoose-auto-increment": "^5.0.33",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14.14.41",
        "@types/socket.io": "^2.1.13",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "apidoc": "^0.20.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.5.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-register": "^6.26.0",
        "dotenv": "^7.0.0",
        "eslint": "^6.6.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jest": "^22.4.1",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-watch": "^5.0.1",
        "husky": "^3.0.9",
        "jest": "^24.5.0",
        "lint-staged": "^9.4.2",
        "nodemon": "^1.18.10",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    },
    "babel": {
        "presets": [
            [
                "env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ],
        "plugins": [
            "transform-object-rest-spread",
            "transform-class-properties"
        ]
    },
    "jest": {
        "testEnvironment": "node"
    }
}
